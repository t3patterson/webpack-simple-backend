{
  "name": "webpack-es6",
  "version": "1.0.0",
  "description": "starter kit for webpack-es6",
  "main": "app.js",
  "scripts": {
    "init-project": "npm run remove-origin && npm run set-project-name && install",
    "remove-origin": "git remote remove origin || true",
    "start": "node --use_strict server.js",
    "go": "npm-run-all --parallel webpack-watcher dev-server",
    "dev-server": "NODE_ENV=development nodemon server.js --watch src --watch server.js --ignore src/clientApp",
    "webpack-watcher": "webpack --progress --watch --config webpack.config.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "set-project-name": "node ./src/cli/setProjectName.js",
    "build-dist": "webpack",
    "seed:dev": "NODE_ENV=development node --use_strict ./src/db/seed.js",
    "seed:prod": "node --use_strict ./src/db/seed.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.15.3",
    "backbone": "1.3.3",
    "body-parser": "1.16.1",
    "ejs": "2.4.2",
    "express": "4.14.0",
    "jquery": "2.2.4",
    "mongoose": "4.8.3",
    "npm-run-all": "4.0.1",
    "react": "15.1.0",
    "react-dom": "15.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "0.11.2",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "prompt": "1.0.0",
    "resolve-url-loader": "2.0.0",
    "sass-loader": "5.0.1",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.8.4"
  }
}
